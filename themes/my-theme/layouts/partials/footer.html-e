
	<footer role="contentinfo">
		<div class="hr"></div>
		<div class="footer-link">
			{{ with .Site.Params.email }}<a href="mailto:{{ . }}" target="_blank"><i class="fa fa-envelope fa-2x"></i></a>{{ end }}
			{{ with .Site.Params.twitter }}<a href="https://twitter.com/{{ . }}" target="_blank"><i class="fa fa-twitter fa-2x"></i></a>{{ end }}
			{{ with .Site.Params.facebook }}<a href="https://www.facebook.com/{{ . }}" target="_blank"><i class="fa fa-facebook fa-2x"></i></a>{{ end }}
			{{ with .Site.Params.github }}<a href="https://github.com/{{ . }}" target="_blank"><i class="fa fa-github fa-2x"></i></a>{{ end }}
			{{ with .Site.Params.linkedin }}<a href="https://linkedin.com/{{ . }}" target="_blank"><i class="fa fa-linkedin fa-2x"></i></a>{{ end }}
      {{ if .RSSlink }}
      <a href="{{ .RSSlink }}"><i class="fa fa-rss fa-2x"></i></a>
      {{ end }}
		</div>
		<div class="copyright">Copyright &copy; {{ .Site.Copyright | safeHTML }}</div>
	</footer>

  {{ template "_internal/google_analytics.html" . }}

</div>

{{ with .Site.GoogleAnalytics }}<script>
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
	ga('create', '{{ . }}', 'auto');
	ga('send', 'pageview');
</script>{{ end }}

<script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js"></script>
<script>hljs.initHighlightingOnLoad();</script>

</body>
</html>
